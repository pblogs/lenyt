= render 'assets'
%section.content
  = form_for @product, url: listing_path(@product), html: {method: :put, multipart: true } do |f|
    = render 'shared/errors', resource: @product
    = render partial: 'form', locals: {f: f}
    = f.submit "Update product", class: 'list-it col-xs-12 col-sm-12 col-md-12 col-lg-12 btn'
